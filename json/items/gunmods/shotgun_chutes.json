    ],

    "name": {
      "str": "speedloader chute for 6-round combat shotguns",
      "str_pl": "speedloader chutes for 6-round combat shotguns"
    },
    "description": "A small metal ramp that mounts just behind the loading port of a shotgun.  It allows one to reload their shotgun faster, either by hand or through use of speedloader tubes.",

    "variant_type": "gun",
    "variants": [
      {
        "id": "steel",

        "name": {
          "str": "Arredondo chute for Mossberg 500",
          "str_pl": "Arredondo chutes for Mossberg 500"
